<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/bundles/SettingsBundle")
    @Scripts.Render("~/bundles/scriptSettings")
    <link rel="stylesheet" type="text/css" href="" />

    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/fullcalendar")

    <style>
        #LogInLink {
            float: right;
        }
    </style>



</head>

<body>
    <br />
    <br />
    <br />
    <br />


    <div>

        <div class="navbar navbar-inverse navbar-fixed-top">

            <div id="LogInLink">
                @Html.ActionLink("Log In", "LogIn", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="container">
                <div class="navbar-header">

                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("Home", "Start", "Home", null, new { @class = "navbar-brand" })

                    @Html.ActionLink("Make a Booking!", "Index", "Home", null, new { @class = "navbar-brand" })

                    <div class="vit" style="position:absolute;top:0;left:0;width:100%;height:100%;background-color:white"></div>
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav"></ul>
                </div>
            </div>

        </div>
    </div>
    <div class="container body-content">

        @RenderBody()




    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/fullcalendar")
    @Scripts.Render("~/bundles/SettingsBundle")
    @Scripts.Render("~/bundles/bootstrap")


    @RenderSection("scripts", required: false)

    <script>


        $(document).ready(function () {
            getActiveCss();

            $(".vit").fadeOut(600);
        });



    </script>

</body>

</html>
